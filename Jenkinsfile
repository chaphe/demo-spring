      steps {
        echo 'This is the building phase'
      }
    }
    
    stage("test") {
      steps {
        echo 'This is the test phase'
      }
    }
    
    stage("deploy") {
      steps {
        echo 'This is the deploy phase'
      }
    }
  }
}
    
